@{
}
<link rel="stylesheet" href="~/css/indexHome.css" asp-append-version="true" />

<h3>@ViewBag.Titulo</h3>
<p>Estos son nuestros clientes:</p>
<ul>
	@foreach (string s in Model)
	{
		<li>@s</li>
	}
</ul>
<div class="card">
	<a asp-controller="Propietarios" asp-action="Index">🏠 Propietarios</a>
</div>
<div class="card">
	<a asp-controller="Inquilinos" asp-action="Index">👨‍👩‍👧‍👦 Inquilinos</a>
</div>

<div class="imgIndexHome">
	<img src="~/img/fondo-Index-Home.jpg" alt="Inmobiliaria" title="Inmobiliaria">
</div>


